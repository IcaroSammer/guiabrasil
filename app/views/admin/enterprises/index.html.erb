<%= title_page "Empresas " %>
<%= add_resource "Empresa", new_admin_enterprise_path %>
<%= render partial: 'layouts/filters', locals: { path: filters_admin_enterprises_path, resource: Enterprise } %>


<table class="table-page">
  <thead>
    <tr>
      <th><%= Profile.human_attribute_name(:name) %></th>
      <th><%= Enterprise.human_attribute_name(:email) %></th>
      <th><%= Enterprise.human_attribute_name(:phone_number) %></th>
      <th><%= Profile.human_attribute_name(:active) %></th>
      <th>&nbsp;</th>
    </tr>
  </thead>
  <tbody>
    <%= display_list_for(@enterprises, fields: 4) %>
  </tbody>
</table>

<%= will_paginate @enterprises %>
