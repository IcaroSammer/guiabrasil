<%= title_page "Items de Conforto" %>

<%= add_resource "Item de Conforto", new_admin_commfort_path %>

<%= render partial: 'layouts/filters', locals: { path: filters_admin_commforts_path, resource: Commfort } %>



<table class='table-page'>
  <thead>
    <tr>
      <th><%= Commfort.human_attribute_name(:name) %></th>
      <th><%= Commfort.human_attribute_name(:active) %></th>
      <th>&nbsp;</th>
    </tr>
  </thead>
  <tbody>
    <%= display_list_for(@commforts, fields: 3) %>
  </tbody>
</table>

<%= will_paginate @commforts %>
