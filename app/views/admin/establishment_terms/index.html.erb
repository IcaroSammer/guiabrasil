
<%= title_page "Termos de Estabelecimentos" %>

<%= add_resource "Termo de Establishmento", new_admin_establishment_term_path %>

<%= render partial: 'layouts/filters', locals: { path: filters_admin_establishment_terms_path, resource: Term } %>

<table class="table-page">
  <thead>
    <tr>
      <th><%= EstablishmentTerm.human_attribute_name(:name) %></th>
      <th><%= EstablishmentTerm.human_attribute_name(:description) %></th>
      <th><%= EstablishmentTerm.human_attribute_name(:type_term) %></th>
      <th><%= EstablishmentTerm.human_attribute_name(:publication_date) %></th>
      <th><%= EstablishmentTerm.human_attribute_name(:inactivated_date) %></th>
      <th><%= EstablishmentTerm.human_attribute_name(:active) %></th>
      <th>&nbsp;</th>
    </tr>
  </thead>
  <tbody>
    <%= display_list_for(@establishment_terms, fields: 3) %>
  </tbody>
</table>

<%= will_paginate @terms %>

       