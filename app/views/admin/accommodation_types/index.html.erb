<%= title_page "Categorias de Acomodação" %>

<%= add_resource "Categoria de Acomodação", new_admin_accommodation_type_path %>

<%= render partial: 'layouts/filters', locals: { path: filters_admin_accommodation_types_path, resource: AccommodationType } %>



<table class='table-page'>
  <thead>
    <tr>
      <th><%= AccommodationType.human_attribute_name(:name) %></th>
      <th><%= AccommodationType.human_attribute_name(:description) %></th>
      <th><%= AccommodationType.human_attribute_name(:active) %></th>
      <th>&nbsp;</th>
    </tr>
  </thead>
  <tbody>
    <%= display_list_for(@accommodation_types, fields: 3) %>
  </tbody>
</table>

<%= will_paginate @accommodationtype %>