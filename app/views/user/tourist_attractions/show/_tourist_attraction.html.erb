	<td><%= link_to 'Editar', edit_user_tourist_attraction_path(tourist_attraction) %></td>
	<td><%= link_to 'Inativar', user_tourist_attraction_path(tourist_attraction),
	method: :delete,
	data: { confirm: 'Are you sure?' } %></td>
</tr>
