<%= f.fields_for :establishment do |ff| %>
<div class="card">
	<div class="card-header"><strong>Informações de redes sociais</strong></div>
	<div class="card-body">


		<div class="form-group row">
			<div class="col-md-3">
				<%= ff.input :facebook, class: "form-control" %>
			</div>

			<div class="col-md-3">
				<%= ff.input :instagram, class: "form-control" %>
			</div>

			<div class="col-md-3">
				<%= ff.input :twitter, class: "form-control" %>
			</div>

			<div class="col-md-3">
				<%= ff.input :linkedin, class: "form-control" %>
			</div>

		</div>

		<div class="form-group row">
			<div class="col-md-12">
				<%= ff.input :website, class: "form-control" %>
			</div>
		</div>


	</div>
</div>
<% end %>
