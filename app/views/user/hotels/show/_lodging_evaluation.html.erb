
<div class="card">
  <div class="card-header"><strong>Tipos de Acomodação</strong></div>
  <div class="card-body">
		<div class="row form-group">
			<div class="col-md-12">

				<%  Lodging.where(id: f.object.lodging).each  do |it|  %>
				<i class="far fa-thumbs-up" style="color:green;"></i> <%= it.name %><br>
				<% end %>
							<%= f.input :number_of_accommodations, class: "form-control" , readonly: true %>
			</div>   
		</div>

  </div>
</div>
<hr>